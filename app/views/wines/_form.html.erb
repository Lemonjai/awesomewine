<%= simple_form_for (@wine) do |f| %>

  <img id="img_prev" width=300 height=300 src="#" alt="your image" class="img-thumbnail hidden"/> <br/>
	<%= f.file_field :image, label: "Pictures",id: "image-upload"  %>
  <%= f.hidden_field :image_cache %>

	<%= f.input :name, label: "Name" %>
	<%= f.input :manufacture, label: "Manufacture" %>
	<%= f.input :city, label: "City" %>
	<%= f.input :year, label: "Year" %>
	<%= f.input :country, label: "Country" %>
	<%= f.input :summary, label: "Summary" %>
	<%= f.input :description, label: "Description" %>
	<br/>
	<%= f.submit "Add Article" %>
<% end %>

